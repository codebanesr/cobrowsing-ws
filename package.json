{
  "name": "surfly-clone",
  "version": "1.0.0",
  "description": "Web-based co-browsing service like Surfly",
  "main": "server.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "http-server public -p 3001 -c-1",
    "start": "node server.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "cors": "^2.8.5",
    "axios": "^1.5.0",
    "cheerio": "^1.0.0-rc.12",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0",
    "http-server": "^14.1.1"
  }
}